{
  "name": "abplanner",
  "version": "1.0.0",
  "description": "A TypeScript library for statistical analysis and experiment planning, designed to streamline AB testing and experiment planning.",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint 'src/**/*.{ts,js}' 'tests/**/*.{ts,js}'",
    "lint:fix": "eslint 'src/**/*.{ts,js}' 'tests/**/*.{ts,js}' --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}' 'tests/**/*.{ts,js,json,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,js,json,md}' 'tests/**/*.{ts,js,json,md}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KanHarI/ABPlanner.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KanHarI/ABPlanner/issues"
  },
  "homepage": "https://github.com/KanHarI/ABPlanner#readme",
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.10.10",
    "@vitest/coverage-v8": "^3.0.4",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vitest": "^3.0.4"
  },
  "dependencies": {
    "cephes": "^2.0.0"
  }
}
